<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@+id/navigation_main">

    <fragment
        android:id="@+id/navigation_main"
        android:name="com.example.thingder.fragments.main.MainFragment"
        android:label="@string/nav_main_title"
        tools:layout="@layout/fragment_main">
    </fragment>

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.example.thingder.fragments.list.ListFragment"
        android:label="@string/nav_list_title"
        tools:layout="@layout/fragment_list">
    </fragment>

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.thingder.fragments.account.AccountFragment"
        android:label="@string/nav_account_title"
        tools:layout="@layout/fragment_account">
    </fragment>

</navigation>